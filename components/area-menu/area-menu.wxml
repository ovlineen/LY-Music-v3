<!--components/area-menu/area-menu.wxml-->
<scroll-view enhanced show-scrollbar="{{false}}" scroll-x enable-flex style="flex-direction: row;">
  <view class="songItem">
    <block wx:for="{{menuData}}" wx:key="id">
      <menu-item data-id="{{item.id}}" itemData="{{item}}"></menu-item>
    </block>
  </view>
</scroll-view>