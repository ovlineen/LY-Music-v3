/* components/ranking-item/ranking-item.wxss */
.ranking {
  display: flex;
  justify-content: space-between;
  align-items: center;
  background-color: #f2f2f2;
  padding-left: 10rpx;
}

.cover {
  width: 250rpx;
  height: 250rpx;
}

.cover .pic {
  width: 100%;
  height: 100%;
}
.song .name {
  margin: 5rpx 10rpx;
}
.song .item {
  font-size: 28rpx;
  margin-bottom: 5rpx;
}
